{% for item in content %}
{% createphp item as="rdf" noautotag %}
<div {{ createphp_attributes(rdf) }}>
	<div class="row">
		<div class="col-md-12">
			<a href="{{ path('cms_content_homepage', {'id': menuKey.id, '_locale': app.request.locale, 'contentid': item.id }) }}"><h4 style="display: inline-block; padding-bottom: 10px; font-weight: normal" class="nomargin orangelink" {{ createphp_attributes( rdf.contentTitle ) }}>{{ createphp_content( rdf.contentTitle ) }}</h4></a>
            {% if is_granted('ROLE_ADMIN') %}
                <a href="{{ path('delete_content_item', {'itemid': item.id, 'language': app.request.locale, 'menuKeyid': menuKey.id }) }}" class="confirm"><img src="/bundles/images/delete_icon.png" style="margin-left: 7px; vertical-align: middle; margin-bottom: 3px" class="inlineblock" width="10px" /></a>
                {% if item.published %}
                    <a href="{{ path('cms_content_unpublish_content', {'contentid': item.id, 'menuKeyid': menuKey.id, 'language': app.request.locale }) }}" ><img src="/bundles/images/eye_icon.png" style="margin-left: 7px; vertical-align: middle; margin-bottom: 3px" class="inlineblock" width="12px" /></a>
                {% else %}
                    <a href="{{ path('cms_content_publish_content', {'contentid': item.id, 'menuKeyid': menuKey.id, 'language': app.request.locale }) }}" ><img src="/bundles/images/closed_eye_icon.png" style="margin-left: 7px; vertical-align: middle; margin-bottom: 3px" class="inlineblock" width="12px" /></a>
                {% endif %}
            {% endif %}
		</div>
	</div>
</div>
{% endcreatephp %}
{% endfor %}