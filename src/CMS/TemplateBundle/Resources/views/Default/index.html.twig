<html lang="en">
<!--<![endif]-->
	<head>
		<meta charset="utf-8">
		<title>OWC</title>
		<meta name="description" content="Shiatsu 3de jaar, Antwerpen OWC ... Dans je leven, Antwerpen OWC.">
		<meta name="author" content="Oost-West Centrum Antwerpen">

		<!-- Mobile Meta -->
		<meta name="viewport" content="width=device-width, initial-scale=1.0">

		<!-- Favicon -->
		<link rel="shortcut icon" href="/bundles/template/images/favicon.ico">

		<!-- Web Fonts -->
		<link href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,700italic,400,700,300&amp;subset=latin,latin-ext' rel='stylesheet' type='text/css'>
		<link href='http://fonts.googleapis.com/css?family=PT+Serif' rel='stylesheet' type='text/css'>

		<!-- Bootstrap core CSS -->
		<link href="/bundles/template/bootstrap/css/bootstrap.css" rel="stylesheet">

		<!-- Font Awesome CSS -->
		<link href="/bundles/template/fonts/font-awesome/css/font-awesome.css" rel="stylesheet">

		<!-- Fontello CSS -->
		<link href="/bundles/template/fonts/fontello/css/fontello.css" rel="stylesheet">

		<!-- Plugins -->
		<link href="/bundles/template/plugins/rs-plugin/css/settings.css" media="screen" rel="stylesheet">
		<link href="/bundles/template/plugins/rs-plugin/css/extralayers.css" media="screen" rel="stylesheet">
		<link href="/bundles/template/plugins/magnific-popup/magnific-popup.css" rel="stylesheet">
		<link href="/bundles/template/css/animations.css" rel="stylesheet">
		<link href="/bundles/template/plugins/owl-carousel/owl.carousel.css" rel="stylesheet">

		<!-- iDea core CSS file -->
		<link href="/bundles/template/css/style.css" rel="stylesheet">

		<!-- Color Scheme (In order to change the color scheme, replace the red.css with the color scheme that you prefer)-->
		<link href="/bundles/template/css/skins/orange.css" rel="stylesheet">

		<!-- Custom css -->
		<link href="/bundles/template/css/custom.css" rel="stylesheet">

		<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
		<!--[if lt IE 9]>
			<script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
			<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>

		<![endif]-->
        <script src='https://www.google.com/recaptcha/api.js'></script>
    </head>

	<!-- body classes: 
			"boxed": boxed layout mode e.g. <body class="boxed">
			"pattern-1 ... pattern-9": background patterns for boxed layout mode e.g. <body class="boxed pattern-1"> 
	-->
	<body class="front no-trans boxed pattern-2">
		<!-- scrollToTop -->
		<!-- ================ -->
		<div class="scrollToTop"><i class="icon-up-open-big"></i></div>

		<!-- page wrapper start -->
		<!-- ================ -->
		<div class="page-wrapper">
		<div class="header-top">
				<div class="container">
					<div class="row">
						<div class="col-xs-2 col-sm-6">

							<!-- header-top-first start -->
							<!-- ================ -->
							<div class="header-top-first clearfix">
								<ul class="social-links clearfix hidden-xs">
									<li class="{% if app.request.get('_locale') == 'nl' %}activelanguagelink{% endif %} languagelink"><a href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge({'_locale': 'nl'}|merge(app.request.query.all))) }}">NL</a></li>
									<li class="{% if app.request.get('_locale') == 'fr' %}activelanguagelink{% endif %} languagelink"><a href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge({'_locale': 'fr'}|merge(app.request.query.all))) }}">FR</a></li>
									<li class="{% if app.request.get('_locale') == 'en' %}activelanguagelink{% endif %} languagelink"><a href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge({'_locale': 'en'}|merge(app.request.query.all))) }}">EN</a></li>
								</ul>
								<div class="social-links hidden-lg hidden-md hidden-sm">
									<div class="btn-group dropdown">
										<button type="button" class="btn dropdown-toggle" data-toggle="dropdown"><i class="fa fa-share-alt"></i></button>
										<ul class="dropdown-menu dropdown-animation">
                                            <li class="{% if app.request.get('_locale') == 'nl' %}activelanguagelink{% endif %} languagelink"><a href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge({'_locale': 'nl'}|merge(app.request.query.all))) }}">NL</a></li>
                                            <li class="{% if app.request.get('_locale') == 'fr' %}activelanguagelink{% endif %} languagelink"><a href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge({'_locale': 'fr'}|merge(app.request.query.all))) }}">FR</a></li>
                                            <li class="{% if app.request.get('_locale') == 'en' %}activelanguagelink{% endif %} languagelink"><a href="{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')|merge({'_locale': 'en'}|merge(app.request.query.all))) }}">EN</a></li>
										</ul>
									</div>
								</div>
							</div>
							<!-- header-top-first end -->

						</div>
						<div class="col-xs-10 col-sm-6">

							<!-- header-top-second start -->
							<!-- ================ -->
							<div id="header-top-second" class="clearfix">

								<!-- header top dropdowns start -->
								<!-- ================ -->
								<div class="header-top-dropdown">
									<div class="btn-group dropdown">
                                        {% if app.user %}
                                            <button type="button" class="btn dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i> {{ app.user.username }}</button>
                                            {% include 'CMSUserBundle:Login:Logouttop.html.twig' %}
                                        {% else %}
										    <button type="button" class="btn dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i> Login</button>
                                            {% include 'CMSUserBundle:Login:Logintop.html.twig' %}
                                        {% endif %}
									</div>

								</div>
								<!--  header top dropdowns end -->

							</div>
							<!-- header-top-second end -->

						</div>
					</div>
				</div>
			</div>

			

			<!-- header start classes:
				fixed: fixed navigation mode (sticky menu) e.g. <header class="header fixed clearfix">
				 dark: dark header version e.g. <header class="header dark clearfix">
			================ -->
			<header class="header fixed clearfix dark">
				<div class="container">
					<div class="row">
						<div class="col-md-12">

							<!-- header-right start -->
							<!-- ================ -->
							<div class="header-right clearfix">

								<!-- main-navigation start -->
								<!-- ================ -->
								<div class="main-navigation animated">

									<!-- navbar start -->
									<!-- ================ -->
									<nav class="navbar navbar-default" role="navigation">
										<div class="container-fluid">

											<!-- Toggle get grouped for better mobile display -->
											<div class="navbar-header">
												<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse-1">
													<span class="sr-only">Toggle navigation</span>
													<span class="icon-bar"></span>
													<span class="icon-bar"></span>
													<span class="icon-bar"></span>
												</button>
											</div>

											<!-- Collect the nav links, forms, and other content for toggling -->
											<div class="collapse navbar-collapse" id="navbar-collapse-1">
												{{ render(controller('CMSContentBundle:Menu:parentItems', { 'id': 1, 'menuKey': menuKey })) }}
											</div>

										</div>
									</nav>
									<!-- navbar end -->

								</div>
								<!-- main-navigation end -->

							</div>
							<!-- header-right end -->

						</div>
					</div>
				</div>
			</header>
			<!-- header end -->
			
			<!-- main-container start -->
			<!-- ================ -->
			<section class="main-container">
			<!-- photo bar start -->
			<section class="photobar">
			<div class="row nomargin">
				<div class="col-md-2 nopadding hidden-xs hidden-sm">
					<img src="/bundles/template/images/topimages/1_{{ random(['1.jpg', '2.jpg', '3.jpg', '4.jpg', '5.jpg', '6.jpg']) }}" alt="Image1" width="100%">
				</div>
				<div class="col-md-2 nopadding hidden-xs hidden-sm">
					<img src="/bundles/template/images/topimages/2_{{ random(['1.jpg', '2.jpg', '3.jpg', '4.jpg', '5.jpg', '6.jpg', '7.jpg']) }}" alt="Image1" width="100%">
				</div>
				<div class="col-md-2 nopadding hidden-xs hidden-sm">
					<img src="/bundles/template/images/topimages/3_{{ random(['1.jpg', '2.jpg', '3.jpg', '4.jpg', '5.gif', '6.gif', '7.jpg']) }}" alt="Image1" width="100%">
				</div>
				<div class="col-md-2 nopadding hidden-xs hidden-sm">
					<img src="/bundles/template/images/topimages/4_{{ random(['1.gif', '2.gif', '3.gif', '4.jpg', '5.jpg', '6.jpg', '7.jpg']) }}" alt="Image1" width="100%">
				</div>
				<div class="col-md-2 nopadding hidden-xs hidden-sm">
					<img src="/bundles/template/images/topimages/5_{{ random(['1.jpg', '2.jpg', '3.jpg', '4.jpg', '5.jpg', '6.jpg', '7.jpg']) }}" alt="Image1" width="100%">
				</div>
				<div class="col-md-2 nopadding hidden-xs hidden-sm">
					<img src="/bundles/template/images/topimages/6_{{ random(['1.jpg', '2.jpg', '3.jpg', '4.jpg', '5.jpg', '6.jpg', '7.jpg']) }}" alt="Image1" width="100%">
				</div>
			</div>
			</section>
			<!-- photo bar ends -->
			{% block main %}{% endblock %}
			</section>
			<!-- main-container end -->

			<!-- footer start (Add "light" class to #footer in order to enable light footer) -->
			<!-- ================ -->
			<footer id="footer">
				<!-- .subfooter start -->
				<!-- ================ -->
				<div class="subfooter">
					<div class="container">
						<div class="row">
							<div class="col-xs-6">
								<p>Design by © <a href="http://www.panenco.com" target="_blank">Panenco</a>.</p>
							</div>
                            <div class="col-xs-6 hidden-xs">
                                <nav class="navbar navbar-default" role="navigation">
                                    <div class="collapse navbar-collapse" id="navbar-collapse-2">
                                        <ul class="nav navbar-nav">
                                            <li><a href="{{ path("cms_content_homepage",{'id': 154, '_locale': app.request.locale}) }}">Links</a></li>
                                        </ul>
                                    </div>
                                </nav>
                            </div>
						</div>
					</div>
				</div>
				<!-- .subfooter end -->

			</footer>
			<!-- footer end -->

		</div>
		<!-- page-wrapper end -->

		<!-- JavaScript files placed at the end of the document so the pages load faster
		================================================== -->
		<!-- Jquery and Bootstap core js files -->
		<script type="text/javascript" src="/bundles/template/plugins/jquery.min.js"></script>
		<script type="text/javascript" src="/bundles/template/bootstrap/js/bootstrap.min.js"></script>

		<!-- Modernizr javascript -->
		<script type="text/javascript" src="/bundles/template/plugins/modernizr.js"></script>

		<!-- jQuery REVOLUTION Slider  -->
		<script type="text/javascript" src="/bundles/template/plugins/rs-plugin/js/jquery.themepunch.tools.min.js"></script>
		<script type="text/javascript" src="/bundles/template/plugins/rs-plugin/js/jquery.themepunch.revolution.min.js"></script>

		<!-- Isotope javascript -->
		<script type="text/javascript" src="/bundles/template/plugins/isotope/isotope.pkgd.min.js"></script>

		<!-- Owl carousel javascript -->
		<script type="text/javascript" src="/bundles/template/plugins/owl-carousel/owl.carousel.js"></script>

		<!-- Magnific Popup javascript -->
		<script type="text/javascript" src="/bundles/template/plugins/magnific-popup/jquery.magnific-popup.min.js"></script>

		<!-- Appear javascript -->
		<script type="text/javascript" src="/bundles/template/plugins/jquery.appear.js"></script>

		<!-- Count To javascript -->
		<script type="text/javascript" src="/bundles/template/plugins/jquery.countTo.js"></script>

		<!-- Parallax javascript -->
		<script src="/bundles/template/plugins/jquery.parallax-1.1.3.js"></script>

		<!-- Contact form -->
		<script src="/bundles/template/plugins/jquery.validate.js"></script>

		<!-- Initialization of Plugins -->
		<script type="text/javascript" src="/bundles/template/js/template.js"></script>

		<!-- Custom Scripts -->
		<script type="text/javascript" src="/bundles/template/js/custom.js"></script>
        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>

		{% include "CmfCreateBundle::includecssfiles.html.twig" %}
		{{ render(controller("cmf_create.jsloader.controller:includeJSFilesAction")) }}

        <link rel="stylesheet" href="{{ asset('bundles/adesignscalendar/css/fullcalendar/fullcalendar.css') }}" />
        <script type="text/javascript" src="{{ asset('bundles/adesignscalendar/js/fullcalendar/jquery.fullcalendar.min.js') }}"></script>
        <script type="text/javascript" src="/bundles/Calender/js/calendar-settings.js"></script>

        {% block javascripts %}{% endblock %}

	</body>
</html>
